    public void actionPerformed(ActionEvent e){

        if(q1c1.isSelected()){    
             christian_music += 1;
             classical_music += 1;
                     country += 1;
            electronic_music += 1;
                       metal += 1;
                        rock += 1;
                  folk_music += 1;
                     hip_hop += 1;
                        jazz += 1;
                        jpop += 1;
                        kpop += 1;
                        lofi += 1;
                         opm += 1;
