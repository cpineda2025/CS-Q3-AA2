        }
        else if(classical_music > christian_music && classical_music > country && classical_music > electronic_music && classical_music > metal && classical_music > rock && classical_music > folk_music && classical_music > hip_hop && classical_music > jazz && classical_music > jpop && classical_music > kpop && classical_music > lofi && classical_music > opm && classical_music > opera && classical_music > rap && classical_music > soul_music && classical_music > soundtrack){
