        }
        else if(country > classical_music && country > christian_music && country > electronic_music && country > metal && country > rock && country > folk_music && country > hip_hop && country > jazz && country > jpop && country > kpop && country > lofi && country > opm && country > opera && country > rap && country > soul_music && country > soundtrack){
