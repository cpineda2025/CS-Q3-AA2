    public static void main(String args[]){

        CompSciAA2 compsciaa2 = new CompSciAA2();
        compsciaa2.setLayout(new GridLayout(13, 1));
        compsciaa2.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
        compsciaa2.setSize(1000,600);
        compsciaa2.setLocation(300,300);
        compsciaa2.setVisible(true);
        compsciaa2.getContentPane().setBackground(Color.gray);
        compsciaa2.setResizable(false);
    }
